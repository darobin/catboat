{
  "name": "catboat",
  "type": "module",
  "version": "0.1.0",
  "description": "Content Addressing Toolbox Of Assorted Tools",
  "author": "Robin Berjon <robin@berjon.com>",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc && tsc-alias -p tsconfig.json",
    "postbuild": "esm2cjs --in dist/esm --out dist/cjs -l error",
    "release": "npm run build && release-it",
    "test": "mocha"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/darobin/catboat.git"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "mocha": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {}
  },
  "devDependencies": {
    "@alcalzone/esm2cjs": "^1.1.2",
    "@types/node": "^22.7.6",
    "eslint": "^8.26.0",
    "mocha": "^10.7.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "hash-wasm": "^4.11.0"
  }
}
